{
  "name": "talent",
  "version": "0.0.1",
  "description": "Learners Guild Talent",
  "main": "src/server/server.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server/server.js",
    "start:dev": "nodemon dist/server/server.js",
    "start:react-dev": "webpack -d --watch && webpack-dev-server",
    "build": "rm -rf dist && babel ./src/server/server.js -d dist/ --watch src/",
    "webpack:server": "webpack --debug --devtool=eval --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/talent.git"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "engines": {
    "node": "8.1.2",
    "npm": "5.5.1"
  }
}
